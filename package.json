{
  "name": "kuber-client",
  "version": "3.1.10",
  "description": "Javascript client library for kuber server",
  "main": "src/index.ts",
  "prepublish": "tsc && cp package.json Readme.md ./dist && ",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "compile": "tsc compile",
    "build": "node ./scripts/build.js",
    "publish": "npm run build && cd ./dist && npm publish",
    "test": "jest",
    "mocha": "ts-mocha",
    "version": "npm version --no-git-tag-version",
    "patch": "npm version patch --no-git-tag-version && npm run publish",
    "minorVersion": "npm version minor --no-git-tag-version",
    "majorVersion": "npm version major --no-git-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sireto/kuber-client-js.git"
  },
  "keywords": [
    "kuber",
    "cardano-serialization-lib",
    "cardano",
    "plutus",
    "payment",
    "development"
  ],
  "author": "Sudip Bhattarai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sireto/kuber-jsclient/issues"
  },
  "homepage": "https://github.com/sireto/kuber-jsclient#readme",
  "dependencies": {
    "@types/node": "^22.15.21",
    "axios": "^1.9.0",
    "buffer": "^6.0.3",
    "cbor-rpc": "^0.0.2",
    "cbor-x": "^1.6.0",
    "libcardano": "1.4.32-alpha",
    "libcardano-wallet": "1.4.32-alpha"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "esbuild": "^0.17.8",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-runner-tsc": "^1.6.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.4",
    "typescript": "^4.8.4"
  },
  "files": [
    "src"
  ]
}
