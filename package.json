{
  "name": "kuber-client",
  "version": "3.3.4",
  "description": "Javascript client library for kuber server",
  "main": "src/index.ts",
  "prepublish": "tsc && cp package.json Readme.md ./dist && ",
  "scripts": {
    "tsc": "tsc",
    "compile": "tsc compile",
    "build": "node ./scripts/build.mjs",
    "publish": "npm run build && cd ./dist && npm publish",
    "benchmark": "npx tsx ./src/tests/benchmark/serialBenchmark.ts && npx tsx ./src/tests/benchmark/parallelBenchmark.ts",
    "test": "vitest run --coverage",
    "test:all": "tsx scripts/test-helpers/run-tests-with-cluster-reset.ts",
    "version": "npm version --no-git-tag-version",
    "patch": "npm version patch --no-git-tag-version && npm run publish",
    "minorVersion": "npm version minor --no-git-tag-version",
    "majorVersion": "npm version major --no-git-tag-version",
    "ts-node": "tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sireto/kuber-client-js.git"
  },
  "keywords": [
    "kuber",
    "cardano-serialization-lib",
    "cardano",
    "plutus",
    "payment",
    "development"
  ],
  "author": "Sudip Bhattarai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sireto/kuber-jsclient/issues"
  },
  "homepage": "https://github.com/sireto/kuber-jsclient#readme",
  "dependencies": {
    "axios": "^1.9.0",
    "buffer": "^6.0.3",
    "libcardano": "2.2.8",
    "libcardano-wallet": "2.2.14"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@types/rollup": "^0.54.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.17.8",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "src"
  ],
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
